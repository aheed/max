<engine:UXML xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:engine="UnityEngine.UIElements" xmlns:editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <Style src="project://database/Assets/MainMenuStyles2.uss?fileID=7433441132597879392&amp;guid=b1c0dbb7fab02f627bd41fee53205e69&amp;type=3#MainMenuStyles2" />
    <engine:VisualElement name="Outer" style="flex-grow: 1; justify-content: flex-start; align-content: center; align-items: center; font-size: 24px; color: rgba(200, 200, 0, 0.78);">
