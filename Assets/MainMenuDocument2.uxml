        <engine:VisualElement name="TopRow" class="menu-border" style="flex-grow: 1; flex-direction: row; justify-content: center; height: 30%; min-height: 25%; max-height: 25%; width: 96%;">
            <engine:Button text="&lt;" name="LeftScroll" class="scroll-side-button" />
            <engine:ScrollView name="ScrollView" horizontal-scroller-visibility="Hidden" vertical-scroller-visibility="Hidden" mode="Horizontal" style="width: 98%; height: 95%; justify-content: center; align-content: center; align-self: center;">
